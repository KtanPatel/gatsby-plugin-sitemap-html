    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^3.0.0",
    "cross-env": "^7.0.0",
    "jest": "^29.0.0"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0"
  },
  "files": [
    "gatsby-node.js",
    "src/templates/sitemap.xsl",
